package com.todo.restApi.services;

import java.util.Date;
import java.util.List;

import com.todo.restApi.entity.Todo;

public class TodoDataHardcoded {
	
	
	private static List<Todo> todo;
	private static int idCounter=0;
	
	static {
		todo.add(new Todo(idCounter++,"anju","learning angular",false,new Date()));
		todo.add(new Todo(idCounter++,"anju","learning java",false,new Date()));
		todo.add(new Todo(idCounter++,"anju","visiting delhi",false,new Date()));
		todo.add(new Todo(idCounter++,"anju","playing guitar",false,new Date()));
		
	}
	
	
	
	

}
